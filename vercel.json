{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    {
      "source": "/:all*(svg|jpg|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=9999999999"
        }
      ]
    },
    {
      "source": "/*(svg|jpg|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=9999999999"
        }
      ]
    }
  ]
}
